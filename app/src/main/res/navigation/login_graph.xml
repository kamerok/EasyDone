<?xml version="1.0" encoding="utf-8"?>
<navigation
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/login_graph"
    app:startDestination="@id/loginFragment">
    <fragment
        android:id="@+id/selectBoardFragment"
        android:name="com.kamer.easydone.SelectBoardFragment"
        android:label="fragment_select_board"
        tools:layout="@layout/fragment_select_board"/>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.kamer.easydone.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login">
        <deepLink app:uri="easydone://login"/>
        <action
            android:id="@+id/action_loginFragment_to_selectBoardFragment"
            app:destination="@id/selectBoardFragment" />
    </fragment>
</navigation>